    <!-- Import Services Common for all Starts-->
    <!-- Entity Import Services
         Transacitons are turned off for these services because they set transactions and timeouts inside the services with txTimeout parameter -->
    <service name="entityImportDirectoryForERP" engine="java"
            location="com.ofbiz.importinterface.services.common.ImportServices" invoke="entityImportDirectoryForERP" auth="false" use-transaction="false">
        <description>Imports all entity xml files contained in a directory</description>
        <!-- permission-service service-name="entityMaintPermCheckForVishal" main-action="CREATE"/ -->
        <attribute name="path" type="String" mode="IN" optional="true"/>
        <attribute name="mostlyInserts" type="String" mode="IN" optional="true"/>
        <attribute name="maintainTimeStamps" type="String" mode="IN" optional="true"/>
        <attribute name="createDummyFks" type="String" mode="IN" optional="true"/>
        <attribute name="checkDataOnly" type="String" mode="IN" optional="true"/>
        <attribute name="deleteFiles" type="String" mode="IN" optional="true"/>
        <attribute name="txTimeout" type="Integer" mode="IN" optional="true"/>
        <attribute name="filePause" type="Long" mode="IN" optional="true"/>
        <attribute name="messages" type="List" mode="OUT" optional="false"/>
        <attribute name="bookingId" type="String" mode="INOUT" optional="true"/>
        <attribute name="orderId" type="String" mode="INOUT" optional="true"/>
        <attribute name="jobCardId" type="String" mode="INOUT" optional="true"/>
    </service>
    <service name="parseEntityXmlFileForERP" engine="java"
            location="com.ofbiz.importinterface.services.common.ImportServices" invoke="parseEntityXmlFileForERP" auth="true" use-transaction="false">
        <description>Parses an entity xml file or an entity xml text</description>
        <permission-service service-name="entityMaintPermCheck" main-action="VIEW"/>
        <attribute name="url" type="java.net.URL" mode="IN" optional="true"/>
        <attribute name="xmltext" type="String" mode="IN" optional="true" allow-html="any"/>
        <attribute name="onlyInserts" type="String" mode="IN" optional="true"/>
        <attribute name="maintainTimeStamps" type="String" mode="IN" optional="true"/>
        <attribute name="txTimeout" type="Integer" mode="IN" optional="true"/>
        <attribute name="createDummyFks" type="String" mode="IN" optional="true"/>
        <attribute name="checkDataOnly" type="String" mode="IN" optional="true"/>
        <attribute name="placeholderValues" type="java.util.Map" mode="IN" optional="true"/>
        <attribute name="rowProcessed" type="Long" mode="OUT" optional="false"/>
    </service>
    <!-- Import Services Common for all Ends-->
    <!-- Category Import Starts-->
    <service name="prepareAndImportCategoryXML" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.category.ImportCategoryServices" invoke="prepareAndImportCategoryXML">
        <description>create category xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
    </service>
 
	 
	<service name="importCategoryInERPFromCsv" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.category.ImportCategoryServices" invoke="importCategoryInERPFromCsv">
        <description>import category data from csv</description>
        <implements service="uploadFile"/>
		<attribute name="dumpDirPath" type="String" mode="IN" optional="true"/>
    </service>
    <!-- Category Import Ends-->
    
    <!-- Vehicle Import Starts-->
    <service name="prepareAndImportVehicleXML" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.vehicle.ImportVehicleServices" invoke="prepareAndImportVehicleXML">
        <description>create vehicle xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
    </service>
 
	 
	<service name="importVehicleInERPFromCsv" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.vehicle.ImportVehicleServices" invoke="importVehicleInERPFromCsv">
        <description>import vehicle data from csv</description>
        <implements service="uploadFile"/>
		<attribute name="dumpDirPath" type="String" mode="IN" optional="true"/>
    </service>
	<!-- Vehicle Import Ends-->
    
    <!-- Product Import Starts-->
    <service name="prepareAndImportProductXML" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.product.ImportProductServices" invoke="prepareAndImportProductXML">
        <description>create Product xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
    </service>
 
	 
	<service name="importProductInERPFromCsv" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.product.ImportProductServices" invoke="importProductInERPFromCsv">
        <description>import Product data from csv</description>
        <implements service="uploadFile"/>
		<attribute name="dumpDirPath" type="String" mode="IN" optional="true"/>
    </service>
    <!-- Product Import Ends-->
    
    <!-- PVPrice Import Starts-->
    <service name="prepareAndImportPVPriceXML" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.pvprice.ImportPVPriceServices" invoke="prepareAndImportPVPriceXML">
        <description>create PVPrice xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
		
    </service>
 
	 
	<service name="importPVPriceInERPFromCsv" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.pvprice.ImportPVPriceServices" invoke="importPVPriceInERPFromCsv">
        <description>import PVPrice data from csv</description>
        <implements service="uploadFile"/>
		<attribute name="dumpDirPath" type="String" mode="IN" optional="true"/>
		<override name="uploadFilePath" optional="true"></override>
    </service>
    <!-- PVPrice Import Ends-->

    <!-- Inventory Import Starts-->
    <service name="prepareAndImportProductInventoryXML" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.inventory.ImportProductInventoryServices" invoke="prepareAndImportProductInventoryXMLs">
        <description>create PVPrice xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
		
    </service>
 
	<service name="importProductInventoryFromCsv" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.inventory.ImportProductInventoryServices" invoke="importProductInventoryFromCsv">
        <description>import inventory data from csv</description>
        <implements service="uploadFile"/>
		<attribute name="dumpDirPath" type="String" mode="IN" optional="true"/>
		<override name="uploadFilePath" optional="true"></override>
    </service>
    <!-- Inventory Import Ends-->

    
    <!-- Configurable Product and Item Association Import Starts-->
    <service name="prepareAndImportProductItemAssocXML" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.piassoc.ImportProductItemAssocServices" invoke="prepareAndImportProductItemAssocXML">
        <description>create PVPrice xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
    </service>
	 
	 <service name="importProductItemAssocFromCsv" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.piassoc.ImportProductItemAssocServices" invoke="importProductItemAssocFromCsv">
        <description>import vehicle data from csv</description>
        <implements service="uploadFile"/>
		<attribute name="dumpDirPath" type="String" mode="IN" optional="true"/>
    </service>
    
    <!-- Configurable Product and Item Association Import Ends-->
    
	<service name="importOrderInERPServiceFromCRM" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ImportOrderServices" invoke="importOrderInERPServiceFromCRM">
        <description>import importOrderInERPServiceFromCRM</description>
		<attribute name="orderInERPServiceFromCRM" type="String" mode="IN" optional="false" allow-html="any"/>
    </service>
    
