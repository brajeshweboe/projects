/* === Wish List Page === */

.itemAddToCartButton,
.reorderButton  {
    display: list-item!important;
    text-align: center!important;
}

.showWishlistOrderItemsItemTotal {
    width: 100%;
}


/* === Payment Method === */

.ebsCheckoutImage {
    background: url("http://www.ebs.in/images/logo_ebs.png") no-repeat scroll 50% center;
    display: inline-block;
    height: 90px;
    width: 80px;
}

.ebsCheckoutImage span {
   display: none;
}

.payNetzCheckoutImage {
    background: url("http://www.atomtech.in/images/logo.png") no-repeat scroll 50% center;
    display: inline-block;
    height: 68px;
    width: 129px;
}

.payNetzCheckoutImage span {
   display: none;
}

.payPalCheckoutImage { /* Checkout; Order Summary */
    background: url("https://www.paypal.com/en_US/i/btn/btn_xpressCheckout.gif") no-repeat scroll 50% center;
    display: inline-block;
    height: 42px;
    width: 145px;
}

.payPalCheckoutImage span { /* Checkout; Order Summary */
      display: none;
}

.payPalImage { /* Order Confirmation */
    background: url("/osafe_theme/images/user_content/images/payPalConfirmation.png") no-repeat scroll 50% center;
    display: inline-block;
    height: 23px;
    position: relative;
    top: 7px;
    width: 90px;
}

.payPalImage span { /* Order Confirmation */
   display: none;
}

/* === Multiple Shipping Addresses === */

.container.itemAddress.multiAddressItemAddress {
     max-width: 320px;
}

/* === Previously Purchased Products === */

.ReorderItems_Group_1 {
    float: left;
}

.ReorderItems_Group_2 {
    float: left;
}

.ReorderItems_Group_3 {
    float: right;
}

.itemCheckbox div input {
    display: block;
    margin: 10px auto;
    text-align: center;
}

/* === Error Messages  === */

.content-messages {
    clear: left;
    color: #000000;
    float: left;
    margin: 10px 0;
    padding: 10px;
    width: 100%;
}

.content-messages p {
    float: left;
    font-size: 14px;
    margin: 3px 0 0 5px;
}

.content-messages ul {
    clear: left;
    float: left;
    margin: 5px 0 0 50px;
}

.content-messages ul li {
    margin: 5px 0 0;
}

.eCommerceErrorMessage {
    border: 1px solid #F00;
    background-color: #FBE7E8; /* Light Red */
}

.eCommerceErrorMessage .errorImage {
    background-image: url("/osafe_theme/images/user_content/images/errorIcon.png");
    background-position: 50% center;
    background-repeat: no-repeat;
    float: left;
    height: 24px;
    width: 24px;
}

.eCommerceSuccessMessage {
	border: 1px solid #076B17;
	background-color: #CCFFCC; /* Green */
}

.eCommerceSuccessMessage .eventImage {
    background-image: url("/osafe_theme/images/user_content/images/successIcon.png");
    background-position: 50% center;
    background-repeat: no-repeat;
    float: left;
    height: 24px;
    width: 24px;
}

.eCommerceWarningMessage {
	border: 1px solid #ffcb61;
	background-color: #FFFF99; /* Yellow */
}

.eCommerceWarningMessage .infoImage {
    background-image: url("/osafe_theme/images/user_content/images/warningIcon.png");
    background-position: 50% center;
    background-repeat: no-repeat;
    float: left;
    height: 24px;
    width: 24px;
}

.inError, .inError:focus {
    background: none repeat scroll 0 0 #FBE7E8 !important;
    border: 1px solid #C00000 !important;
}

.fieldErrorMessage {
    display: inline-block;
}

.fieldErrorMessage li {
    color: #FF0000;
    font-size: 12px;
}

.pagingContainer {
    float: left;
    margin: 10px 0 0;
    width: 100%;
}

.paginationContainer {
    float: right;
    font-size: 14px;
}

#pageLink a {
    border-bottom: 1px dotted;
}

#pageLink a:hover {
    border-bottom: 1px solid;
}

#nextPageLink a {
    border-bottom: 1px dotted;
    margin: 0 0 0 5px;
}

#nextPageLink a:hover {
    border-bottom: 1px solid;
}

#prevPageLink a {
    border-bottom: 1px dotted;
    margin: 0 5px 0 0;
} 

#prevPageLink a:hover {
    border-bottom: 1px solid;
}

/* === Dialog Box Styles ===*/

#search_displayDialog {
    max-width: 600px !important;
}

#storePickup_displayDialog {
    max-width: 900px !important;
}

#pincodeChecker_displayDialog {
    max-width: 600px !important;
}

#confirm_displayDialog {
    max-width: 600px !important;
}

/* === Static Page and User Content Styles === */

.staticContent p {
    display: block;
    margin: 0 0 20px;
}

.staticContent ul,
.termsAndConditions ul,
.longDescription ul, 
.deliveryInfo ul,
.salesPitch ul,
.ingredients ul,
.specialInstructions ul,
.warnings ul {
    list-style: disc inside none;
}

.staticContent ul li ul,
.termsAndConditions ul li ul,
.longDescription ul li ul, 
.deliveryInfo ul li ul,
.salesPitch ul li ul
.ingredients ul li ul,
.specialInstructions ul li ul,
.warnings ul  li ul {
    list-style: circle inside none;
    margin: 0 0 0 15px;
}

/* === Footer Styles === */

#eCommerceFooter {
    background: url("/osafe_theme/images/user_content/images/tornPaperBackgroundRight.jpg") repeat-y top right;
    float: left;
    position: relative;
    width: 100%; 
}

.footerColumn {
    float: left;
    margin: 0 0 20px;
    min-width: 150px;  /* Columns will wrap after they collapse beyond 150px */
    padding: 0 0 0 10px;
    width: 20%;
}

.footerColumn h3 {
    font-family: gothamRounded, verdana;
    font-size: 20px;
}

.footerLinks li {
    margin: 10px 0 0;
}

/* === Built By === */

#eCommerceBuiltByBevelWrapper{
    background: url("/osafe_theme/images/user_content/images/tornPaperBackgroundRight.jpg") repeat-y scroll right top transparent;
    float: left;
    padding: 0 15px;
    width: 100%;
}

#eCommerceBuiltBy {
    float: right;
    padding: 10px;
}

#eCommerceBuiltBy p {
    color: #aaaaaa;
    font-size: 11px;
    text-align: right;
}

#eCommerceBuiltBy a {
    color: #aaaaaa;
    font-size: 11px;
}


