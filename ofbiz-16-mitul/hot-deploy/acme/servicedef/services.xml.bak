<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="http://ofbiz.apache.org/dtds/services.xsd">
    <description>Gaadizo Services</description>
    <vendor></vendor>
    <version>1.0</version>

    <service name="noSyntaxError" engine="interface">
        <description>
            Dummy service to prevent empty files and syntax error - Remove when the 1st real service will be added here 
        </description>
    </service> 
    <service name="prepareAndImportOrderXML" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ImportOrderServices" invoke="prepareAndImportOrderXML">
        <description>create order xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
    </service>
 
	  <service name="uploadFile" engine="java" location="com.ofbiz.importinterface.services.customer.ImportCompanyServices" invoke="uploadFile" auth="true" transaction-timeout="720000">
        <description>Upload the file to runtime upload temp directory</description>
        <implements service="uploadFileInterface"/>
        <attribute name="uploadFileName" mode="INOUT" type="String" optional="true"/>
        <attribute name="uploadedFile" type="java.nio.ByteBuffer" mode="IN" optional="true"/>
        <attribute name="userLogin" type="org.ofbiz.entity.GenericValue" mode="IN" optional="true"/>
        <attribute name="uploadFilePath" mode="OUT" type="String" optional="false"/>
    </service>
	<service name="importOrderInERPService" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ImportOrderServices" invoke="importOrderInERPService">
        <description>create order xml from csv</description>
		<attribute name="dumpDirPath" type="String" mode="IN" optional="true"/>
    </service>
    
    <service name="exportOrderStatusToFMO" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ExportOrderStatusToFMO" invoke="exportOrderStatusToFMO">
        <description>export order status </description>
		<attribute name="ordersWithStatus" type="String" mode="IN" optional="false"/>
    </service>
     <service name="exportOrderItemStatusToFMO" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ExportOrderStatusToFMO" invoke="exportOrderItemStatusToFMO">
        <description>export order Item status  </description>
		<attribute name="orderItemStatus" type="Map" mode="IN" optional="false"/>
    </service>
	<service name="sendMailsOnJobSheetCreation" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.services.OmsServices" invoke="sendMailsOnJobSheetCreation">
        <description>send Mails On JobSheet Creation</description>
		<attribute name="picklistId" type="String" mode="IN" optional="false"/>
		<attribute name="userLogin" mode="IN" type="org.ofbiz.entity.GenericValue" optional="true"/>
    </service>
	<service name="entityImportDirectory" engine="java"
            location="com.ofbiz.importinterface.services.order.ImportOrderServices" invoke="entityImportDirectory" auth="false" use-transaction="false">
        <description>Imports all entity xml files contained in a directory</description>
        <!-- permission-service service-name="entityMaintPermCheckForVishal" main-action="CREATE"/ -->
        <attribute name="path" type="String" mode="IN" optional="true"/>
        <attribute name="mostlyInserts" type="String" mode="IN" optional="true"/>
        <attribute name="maintainTimeStamps" type="String" mode="IN" optional="true"/>
        <attribute name="createDummyFks" type="String" mode="IN" optional="true"/>
        <attribute name="checkDataOnly" type="String" mode="IN" optional="true"/>
        <attribute name="deleteFiles" type="String" mode="IN" optional="true"/>
        <attribute name="txTimeout" type="Integer" mode="IN" optional="true"/>
        <attribute name="filePause" type="Long" mode="IN" optional="true"/>
        <attribute name="messages" type="List" mode="OUT" optional="false"/>
    </service>
       
