    background: url("/osafe_theme/images/user_content/images/2shipping-on.png") no-repeat scroll 50% 0;
}

#eCommerceCartFootprintContainer li#shippingMethod.on,
#eCommerceCartFootprintContainer li#shippingMethod.next,
