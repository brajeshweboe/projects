     <service name="importCompanyInERPFromCsv" engine="java" auth="true"
            location="com.ofbiz.importinterface.services.customer.ImportCompanyServices" invoke="importCompanyInERPFromCsv">
        <description>Import a CSV (name,value) of AddressMatchMap records</description>
         <implements service="uploadFile"/>
         <override name="uploadFilePath" optional="true"></override>
    </service>
     <service name="importCompanyInERPFromCsv" engine="java" auth="true"
            location="com.ofbiz.importinterface.services.customer.ImportCompanyServices" invoke="importCompanyInERPFromCsv">
        <description>Import a CSV (name,value) of AddressMatchMap records</description>
         <implements service="uploadFile"/>
         <override name="uploadFilePath" optional="true"></override>
    </service>
    
     <service name="exportServiceFromOutSide" engine="simple" auth="true" transaction-timeout="3000" export="true"
            location="component://gaadizo/minilang/nextsequence/SequenceServices.xml" invoke="exportServiceFromOutSide">
        <description>update Customer JobCard</description>
        <attribute name="registrationNumber" type="String" mode="IN" optional="true"/>
                
    </service>
    <service name="importChartOfAccountInERPFromCsv" engine="java" auth="true"
            location="com.ofbiz.importinterface.services.customer.ImportCompanyAccountServices" invoke="importChartOfAccountInERPFromCsv">
        <description>Import a CSV (name,value) of Company Accounts In ERP records</description>
         <implements service="uploadFile"/>
         <override name="uploadFilePath" optional="true"></override>
    </service>
    
    <service name="importOrderInERPFromRestApi" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ImportOrderServicesJson" invoke="importOrderInERPFromRestApi">
        <description>JSON storing in DB and other logging stuff.</description>
		<attribute name="orderBookingJsonFromCRM" type="String" mode="IN" optional="false" allow-html="any"/>
    </service>
    <service name="importBookingListInERPFromRestApi" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ImportOrderServicesJson" invoke="importBookingListInERPFromRestApi">
        <description>import importOrderInERPFromRestApi</description>
		<!-- <attribute name="orderBookingJsonFromCRM" type="String" mode="IN" optional="false" allow-html="any"/> -->
		<attribute name="output1" type="String" mode="OUT" optional="true"/>
    </service>
    <service name="importOrderInERPFromJson" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ImportOrderServiceInERP" invoke="importOrderInERPFromJson">
        <description>OfBiz code call starts and Logging in file system in form error CSV gets done.</description>
		<attribute name="bookingOrderPojoData" type="com.ofbiz.importinterface.services.order.BookingOrderPojo" mode="IN" optional="false" allow-html="any"/>
		<attribute name="orderId" type="String" mode="OUT" optional="true"/>
		<attribute name="bookingId" type="String" mode="OUT" optional="true"/>
		<attribute name="status" type="String" mode="OUT" optional="true"/>
    </service>
    
    <service name="prepareAndImportOrderXMLFromJson" engine="java" transaction-timeout="7200" auth="true"
            location="com.ofbiz.importinterface.services.order.ImportOrderServiceInERP" invoke="prepareAndImportOrderXMLFromJson">
        <description>All Services call and JSON conversion in OFBiz XML.</description>
		<attribute name="booking" type="Object" mode="IN" optional="false" allow-html="any"/>
		<attribute name="dumpDirPath" type="String" mode="OUT" optional="true"/>
		<attribute name="success" type="String" mode="OUT" optional="true"/>
		<attribute name="orderId" type="String" mode="OUT" optional="true"/>
		<attribute name="bookingId" type="String" mode="OUT" optional="true"/>
		<attribute name="status" type="String" mode="OUT" optional="true"/>
    </service>
     <service name="createPartyPostalAddress" engine="simple"
            location="component://party/minilang/contact/PartyContactMechServices.xml" invoke="createPartyPostalAddress" auth="true">
        <description>Create a Postal Address</description>
        <permission-service service-name="partyContactMechPermissionCheck" main-action="CREATE"/>
        <auto-attributes entity-name="PartyContactMech" include="all" mode="IN" optional="true"/>
        <auto-attributes entity-name="PostalAddress" include="nonpk" mode="IN" optional="true"/>
        <attribute name="paymentMethodId" type="String" mode="IN" optional="true"/>
        <attribute name="contactMechPurposeTypeId" type="String" mode="IN" optional="true"/>
        <attribute name="contactMechId" type="String" mode="OUT" optional="false"/>
        <override name="address1" optional="false"/>
        <override name="city" optional="false"/>
    </service>
</services>