	<entity entity-name="ProductVehiclePrice" package-name="org.apache.ofbiz.product.product" never-cache="false" 
		title="Vehicle Details">
		<field name="productId" type="id-ne"></field>
		<field name="vehicleId" type="id-ne"></field>
		<field name="productStoreId" type="id-ne"></field>
		<field name="facilityId" type="id-ne"></field>
		<field name="currencyUomId" type="id-ne"/>
		<field name="fromDate" type="date-time" />
		<field name="thruDate" type="date-time" />
		<field name="mrp" type="currency-amount"></field>
		<field name="price" type="currency-amount"></field>
		<field name="customPriceCalcService" type="id"></field>
		<field name="discount" type="currency-amount"></field>
		<field name="workshopPrice" type="currency-amount"></field>
		<field name="taxPercentage" type="currency-amount"></field>
		<field name="taxAmount" type="currency-amount"></field>
		<field name="description" type="value"></field>
		<field name="field1" type="value"></field>
		<field name="field2" type="value"></field>
		<field name="field3" type="currency-amount"></field>
		<field name="field4" type="currency-amount"></field>
		<prim-key field="productId" />
		<prim-key field="vehicleId" />
		<prim-key field="facilityId" />
		<prim-key field="fromDate" />
		<relation type="one" rel-entity-name="Product">
			<key-map field-name="productId" />
		</relation>
		<relation type="one" rel-entity-name="Vehicle">
			<key-map field-name="vehicleId" />
		</relation>
		<relation type="one" fk-name="OPARTY_RATE_CUOM" title="Currency" rel-entity-name="Uom">
            <key-map field-name="currencyUomId" rel-field-name="uomId"/>
          </relation>
          <relation type="one" fk-name="PROD_PRICE_CMET" rel-entity-name="CustomMethod">
        <key-map field-name="customPriceCalcService" rel-field-name="customMethodId"/>
      </relation>
	</entity>
